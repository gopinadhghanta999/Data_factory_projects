{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kasier-adf-dev"
		},
		"kaiser_adb_ls_azureconnect_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'kaiser_adb_ls_azureconnect'"
		},
		"kaiser_databricks_ls_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'kaiser_databricks_ls'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kaiser-healthcare-kv.vault.azure.net/"
		},
		"hosp_sqldb_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "kaisersqlserver.database.windows.net"
		},
		"hosp_sqldb_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().db_name}"
		},
		"hosp_sqldb_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "kasier"
		},
		"kaiser_adb_ls_azureconnect_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1218-053712-1arcelrf"
		},
		"kaiser_adbricks_ls_kv_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1218-053712-1arcelrf"
		},
		"kaiser_hc_kv_ls_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kaiser-healthcare-kv.vault.azure.net"
		},
		"kaiseradlsdev_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://kaiseradlsdev.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "kasier-adf-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"kaiser_adb_ls_azureconnect_accessToken": {
						"value": "[parameters('kaiser_adb_ls_azureconnect_accessToken')]"
					},
					"kaiser_databricks_ls_accessToken": {
						"value": "[parameters('kaiser_databricks_ls_accessToken')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"kaiser_adb_ls_azureconnect_properties_typeProperties_existingClusterId": {
						"value": "[parameters('kaiser_adb_ls_azureconnect_properties_typeProperties_existingClusterId')]"
					},
					"kaiser_hc_kv_ls_properties_typeProperties_baseUrl": {
						"value": "[parameters('kaiser_hc_kv_ls_properties_typeProperties_baseUrl')]"
					},
					"hosp_sqldb_ls_properties_typeProperties_server": {
						"value": "[parameters('hosp_sqldb_ls_properties_typeProperties_server')]"
					},
					"hosp_sqldb_ls_properties_typeProperties_database": {
						"value": "[parameters('hosp_sqldb_ls_properties_typeProperties_database')]"
					},
					"hosp_sqldb_ls_properties_typeProperties_userName": {
						"value": "[parameters('hosp_sqldb_ls_properties_typeProperties_userName')]"
					},
					"kaiser_adbricks_ls_kv_properties_typeProperties_existingClusterId": {
						"value": "[parameters('kaiser_adbricks_ls_kv_properties_typeProperties_existingClusterId')]"
					},
					"kaiseradlsdev_ls_properties_typeProperties_url": {
						"value": "[parameters('kaiseradlsdev_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}